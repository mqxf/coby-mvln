main(agrc: int, argv: list of string): void {
    a: int = 10;
    b: int = 5;
    ?(a > b) {
        print(a);
    }
    &?(b > a) {
        print(b);
    }
    & {
        add(a, b) => printf();
    }

    <b> {
        &(1): print(1);
        &(2): print(2);
        &(5): print(5);
        &: print("Error");
    }

}

add(a: int, b: int): int {
    back a + b;
}
